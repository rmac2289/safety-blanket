{
  "expo": {
    "name": "safety-blanket",
    "slug": "safety-blanket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to give the user appropriate phone numbers for emergency services nearby."
      },
      "bundleIdentifier": "com.rmac2289.safety-blanket"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
